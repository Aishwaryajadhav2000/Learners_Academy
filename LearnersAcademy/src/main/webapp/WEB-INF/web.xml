<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.A.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Demo</servlet-name>
  	<display-name>Demo</display-name>
  	<description></description>
  	<servlet-class>com.simpli.Demo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ClassRooms</servlet-name>
  	<display-name>ClassRooms</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.A.ClassRooms</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Students</servlet-name>
  	<display-name>Students</display-name>
  	<description></description>
  	<servlet-class>com.simplilearn.A.Students</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Demo</servlet-name>
  	<url-pattern>/demo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ClassRooms</servlet-name>
  	<url-pattern>/classRooms</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Students</servlet-name>
  	<url-pattern>/students</url-pattern>
  </servlet-mapping>
</web-app>
